{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning",
      "System.Net.Http": "Warning",
      "Acropolis": "Debug"
    }
  },
  "AllowedHosts": "*",
  "FileStorageOptions": {
    "BaseDirectory": "data/downloads"
  },
  "YoutubeOptions": {
    "ValidUrls": [
      "https://www.youtube.com",
      "https://youtube.com",
      "https://youtu.be"
    ],
    "PreferredQualityValues": [
      "720p60",
      "1080p60"
    ]
  },
  "ConnectionStrings": {
  },
  "RabbitMq": {
    "Host": "rabbitmq",
    "VirtualHost": "/"
  },
  "ScrapeOptions": {
    "IgnoredDomains": [
      "www.youtube.com",
      "youtube.com",
      "youtu.be"
    ]
  },
  "Telegram": {
    "Enabled": false
  }
}
